{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": "3",
        "targets": {
          "browsers": ["last 5 versions", "ie >= 8"]
        }
      }
    ],

    "@babel/preset-react"
  ],
  "plugins": [["@babel/plugin-proposal-class-properties", { "loose": true }]]
}
